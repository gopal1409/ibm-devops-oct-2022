root@ip-172-31-13-26:~# history
sudo -i
    1  apt-get update -y
    2  sudo mkdir -p /etc/apt/keyrings
    3  curl -fsSL https://download.docker.com/linux/ubuntu/gpg | sudo gpg --dearmor -o /etc/apt/keyrings/docker.gpg
    4  echo   "deb [arch=$(dpkg --print-architecture) signed-by=/etc/apt/keyrings/docker.gpg] https://download.docker.com/linux/ubuntu \$(lsb_release -cs) stable" | sudo tee /etc/apt/sources.list.d/docker.list > /dev/null
    5  sudo apt-get update
    6  sudo apt-get install docker-ce docker-ce-cli containerd.io docker-compose-plugin
    7  docker --version
    8  docker volume create sonarqube-conf
    9  docker volume create sonarqube-data
   10  docker volume create sonarqube-logs
   11  docker volume create sonarqube-extensions
   12  ln -s /var/lib/docker/volumes/sonarqube-conf/_data /sonarqube/conf
   13  ln -s /var/lib/docker/volumes/sonarqube-data/_data /sonarqube/data
   14  ln -s /var/lib/docker/volumes/sonarqube-logs/_data /sonarqube/logs
   15  ln -s /var/lib/docker/volumes/sonarqube-extensions/_data /sonarqube/extensions
